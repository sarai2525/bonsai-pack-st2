{
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "extends": [
    "standard",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}
